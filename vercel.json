{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "files": [
    "package.json",
    "package-lock.json",
    "server.js",
    "styles.css",
    "script.js",
    "dashboard.html",
    "daily_brief_2026-02-05.md",
    "daily_brief_2026-02-06.md",
    "project_data.json",
    "memory/2026-02-05.md",
    "research/100x_emerging_gaming_ai_tokens.md",
    "research/ohana_l2_trends.md",
    "research/realest_club_token_gating.md"
  ]
}